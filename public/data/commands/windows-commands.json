{
  "version": "1.0.0",
  "commands": [
    {
      "id": "dir",
      "name": "dir",
      "synopsis": "List directory contents",
      "targets": ["windows"],
      "min_integrity": "user",
      "opsec": {
        "writes_disk": false,
        "noisy": false
      },
      "params": [
        {
          "name": "path",
          "type": "string",
          "default": ".",
          "required": false,
          "flag": ""
        },
        {
          "name": "all",
          "type": "boolean",
          "default": false,
          "required": false,
          "flag": "/a"
        }
      ],
      "preview": "dir [path] [/a]",
      "parser": {
        "type": "table"
      },
      "help": "Display a list of files and subdirectories in a directory."
    },
    {
      "id": "tasklist",
      "name": "tasklist",
      "synopsis": "Display running processes",
      "targets": ["windows"],
      "min_integrity": "user",
      "opsec": {
        "writes_disk": false,
        "noisy": false
      },
      "params": [
        {
          "name": "verbose",
          "type": "boolean",
          "default": false,
          "required": false,
          "flag": "/v"
        }
      ],
      "preview": "tasklist [/v]",
      "parser": {
        "type": "table",
        "shape": ["image_name", "pid", "session_name", "session", "mem_usage"]
      },
      "help": "Display a list of currently running processes."
    },
    {
      "id": "ipconfig",
      "name": "ipconfig",
      "synopsis": "Display IP configuration",
      "targets": ["windows"],
      "min_integrity": "user",
      "opsec": {
        "writes_disk": false,
        "noisy": false
      },
      "params": [
        {
          "name": "all",
          "type": "boolean",
          "default": false,
          "required": false,
          "flag": "/all"
        }
      ],
      "preview": "ipconfig [/all]",
      "parser": {
        "type": "text"
      },
      "help": "Display TCP/IP network configuration values."
    },
    {
      "id": "netstat",
      "name": "netstat",
      "synopsis": "Display network connections",
      "targets": ["windows"],
      "min_integrity": "user",
      "opsec": {
        "writes_disk": false,
        "noisy": false
      },
      "params": [
        {
          "name": "all",
          "type": "boolean",
          "default": false,
          "required": false,
          "flag": "-a"
        },
        {
          "name": "numeric",
          "type": "boolean",
          "default": false,
          "required": false,
          "flag": "-n"
        }
      ],
      "preview": "netstat [-a] [-n]",
      "parser": {
        "type": "table"
      },
      "help": "Display active TCP connections and ports."
    },
    {
      "id": "systeminfo",
      "name": "systeminfo",
      "synopsis": "Display system configuration",
      "targets": ["windows"],
      "min_integrity": "user",
      "opsec": {
        "writes_disk": false,
        "noisy": false
      },
      "params": [],
      "preview": "systeminfo",
      "parser": {
        "type": "text"
      },
      "help": "Display configuration information about a computer and its operating system."
    },
    {
      "id": "whoami",
      "name": "whoami",
      "synopsis": "Display current username",
      "targets": ["windows"],
      "min_integrity": "user",
      "opsec": {
        "writes_disk": false,
        "noisy": false
      },
      "params": [
        {
          "name": "groups",
          "type": "boolean",
          "default": false,
          "required": false,
          "flag": "/groups"
        }
      ],
      "preview": "whoami [/groups]",
      "parser": {
        "type": "text"
      },
      "help": "Display user, group and privileges information."
    }
  ]
}
